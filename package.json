{
	"name": "svelte-pdfjs",
	"version": "0.10.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .prettierignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .prettierignore --write --plugin-search-dir=. .",
		"package": "svelte-package"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.1",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.3.1",
		"pdfjs-dist": "^4.10.38",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.2.0",
		"svelte": "^5.43.14",
		"svelte-check": "^4.0.0",
		"svelte2tsx": "^0.6.27",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"vite": "^6.3.0"
	},
	"exports": {
		".": {
			"svelte": "./dist/index.js",
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./vite": {
			"import": "./dist/utils/vite.js",
			"types": "./dist/utils/vite.d.ts"
		}
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"type": "module",
	"license": "MIT",
	"keywords": [
		"svelte",
		"pdf",
		"pdfjs",
		"pdf.js"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/gtm-nayan/svelte-pdfjs.git"
	},
	"bugs": {
		"url": "https://github.com/gtm-nayan/svelte-pdfjs/issues"
	},
	"dependencies": {
		"esm-env": "^1.0.0"
	},
	"peerDependencies": {
		"pdfjs-dist": "^4.6.82",
		"svelte": "^5"
	},
	"files": [
		"dist",
		"CHANGELOG.md"
	]
}
